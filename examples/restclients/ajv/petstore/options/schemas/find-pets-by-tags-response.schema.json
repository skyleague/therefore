{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FindPetsByTagsResponse",
  "type": "array",
  "items": {
    "$ref": "#/$defs/Pet"
  },
  "$defs": {
    "Pet": {
      "type": "object",
      "properties": {
        "category": {
          "$ref": "#/$defs/Category"
        },
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "photoUrls": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "status": {
          "description": "pet status in the store",
          "enum": ["available", "pending", "sold"]
        },
        "tags": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Tag"
          }
        }
      },
      "required": ["name", "photoUrls"],
      "additionalProperties": true
    },
    "Category": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        }
      },
      "additionalProperties": true
    },
    "Tag": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  }
}
