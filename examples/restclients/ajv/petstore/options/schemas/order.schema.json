{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Order",
  "type": "object",
  "properties": {
    "complete": {
      "type": "boolean"
    },
    "id": {
      "type": "integer"
    },
    "petId": {
      "type": "integer"
    },
    "quantity": {
      "type": "integer"
    },
    "shipDate": {
      "type": "string",
      "format": "date-time"
    },
    "status": {
      "description": "Order Status",
      "enum": ["placed", "approved", "delivered"]
    }
  },
  "additionalProperties": true
}
